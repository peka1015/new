{
    "name": "beispiel",
    "version": "1.0.0",
    "description": "Beispiel",
    "main": "index.html",
    "scripts": {
        "prestart": "npm run prewebpack",
        "start": "npx --no-install webpack-dev-server",
        "start:bs": "ts-node scripts/browsersync",
        "clean": "npx --no-install rimraf dist",
        "dashboard": "webpack-dashboard -- npx --no-install webpack-dev-server",
        "fixme": "ts-node scripts/fixme",
        "tslint": "npx --no-install tslint src/**/*.ts*",
        "prettier": "npx prettier --write src/**/*.ts*",
        "webpackdll": "npx --no-install webpack --config webpack.dll.config.ts --bail",
        "prewebpack": "npm run prettier && npm run webpackdll",
        "webpack": "npx --no-install webpack --profile --bail",
        "prebuild": "npm run prewebpack",
        "build": "npx --no-install webpack --mode production",
        "appserver": "npx json-server --config=config/json-server/config.json config/json-server/daten.js"
    },
    "private": true,
    "homepage": "https://www.hs-karlsruhe.de",
